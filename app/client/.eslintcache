uments/mern-stack-capstone/app/client/src/components/RecordCard/RecordCard.js",["234"],"import React, { useState } from \"react\";\nimport Card from \"@material-ui/core/Card\";\nimport {\n  Box,\n  CardContent,\n  Chip,\n  IconButton,\n  Typography,\n} from \"@material-ui/core\";\nimport useStyles from \"./RecordCardStyles\";\nimport CartIcon from \"../../icons/BoxFull\";\nimport { toCurrencyString } from \"../../util/shop\";\nimport { useCart } from \"../../context/CartContext\";\nimport { ACTIONS } from \"../../context/reducers/cartReducer\";\nimport { useAuth } from \"../../context/AuthContext\";\nimport { useGlobal } from \"../../context/GlobalState\";\nimport RecordModal from \"../RecordModal/RecordModal\";\nimport { Redirect } from \"react-router-dom\";\nimport { parseLabelData, abbreviateTitle } from \"../../util/helpers/recordCardHelpers\";\n\nexport default function RecordCard(props) {\n  const classes = useStyles();\n  const { dispatch } = useCart();\n  const auth = useAuth();\n  const { record } = props;\n  const [recordModalState, setRecordModalState] = useState();\n  const { isSuper } = auth;\n  const {\n    release_title: releaseTitle,\n    artists_sort: artist,\n    styles,\n    image,\n    labels,\n    year,\n    description,\n    review,\n    variations: {\n      stock: { price },\n    },\n    preloved,\n  } = record;\n  const globe = useGlobal();\n  const { setEditRecordId } = globe;\n  const [blur, setBlur] = useState(\"blur(0px)\");\n  const [display, setDisplay] = useState(\"none\");\n  const [editRedirect, setEditRedirect] = useState(false);\n\n  const handleHover = (blurState, displayState) => {\n    if (review.length > 0) {\n      setBlur(blurState);\n\n      setTimeout(() => {\n        setDisplay(displayState);\n      }, 300);\n    }\n  };\n\n  const handleEditClick = () => {\n    setEditRecordId(record._id);\n    setEditRedirect(true);\n  };\n\n  return (\n    <Card className={classes.card} raised>\n      {editRedirect && <Redirect to=\"/dashboard\" />}\n      {recordModalState && (\n        <RecordModal\n          record={record}\n          recordModalState={recordModalState}\n          setRecordModalState={setRecordModalState}\n        />\n      )}\n      <Box\n        onMouseEnter={() => handleHover(\"blur(50px)\", \"block\")}\n        onMouseLeave={() => handleHover(\"blur(0px)\", \"none\")}\n        style={{ position: \"relative\" }}\n      >\n        <img\n          alt=\"record cover\"\n          src={image}\n          className={classes.coverImage}\n          style={{\n            filter: blur,\n            transition: \"0.5s filter\",\n          }}\n        />\n\n        {blur && (\n          <p\n            className={classes.recordReview}\n            style={{\n              display: display,\n            }}\n          >\n            {review}\n          </p>\n        )}\n        {preloved && (\n          <Chip\n            label=\"pre-loved\"\n            size=\"small\"\n            className={classes.preLovedChip}\n          />\n        )}\n        {isSuper() ? (\n          <Chip\n            label=\"edit\"\n            size=\"small\"\n            className={classes.editChip}\n            onClick={() => handleEditClick()}\n          />\n        ) : (\n          <Chip\n            label=\"more info\"\n            size=\"small\"\n            className={classes.moreInfoChip}\n            onClick={() => setRecordModalState(true)}\n          />\n        )}\n      </Box>\n      <CardContent style={{ position: \"relative\", padding: \"2px\" }}>\n        <div className={classes.flexedRow}>\n          <Typography className={classes.artistName}>\n            {abbreviateTitle(artist, 18)}\n          </Typography>\n          <Typography className={classes.recordPrice}>\n            ${toCurrencyString(price)}\n          </Typography>\n        </div>\n        <div className={classes.flexedRow}>\n          <Typography className={classes.recordTitle}>\n            {abbreviateTitle(releaseTitle, 21)}\n          </Typography>\n        </div>\n        <div className={classes.flexedRow}>\n          <Typography className={classes.labelAndYear}>\n            {parseLabelData(labels)} â€¢ {year}\n          </Typography>\n        </div>\n        <div className={classes.flexedRow}>\n          <Typography className={classes.cardGenres}>\n            {styles.length === 1 ? styles[0] : styles[0] + \" / \" + styles[1]}\n          </Typography>\n        </div>\n        <IconButton\n          edge=\"end\"\n          style={{\n            position: \"absolute\",\n            bottom: -4,\n            right: 8,\n          }}\n          onClick={() => {\n            dispatch({\n              type: ACTIONS.ADD_RECORD,\n              payload: record,\n            });\n          }}\n        >\n          <div className={classes.iconContainer}>\n            <span className={classes.addIcon}>ADD</span>\n            <CartIcon\n              className={classes.cartIcon}\n              color=\"secondary\"\n              viewBox=\"0 0 60 60\"\n            />\n          </div>\n        </IconButton>\n      </CardContent>\n    </Card>\n  );\n}\n","/Users/macbook/Documents/mern-stack-capstone/app/client/src/util/shop.js",[],"/Users/macbook/Documents/mern-stack-capstone/app/client/src/components/RecordModal/RecordModal.js",[],"/Users/macbook/Documents/mern-stack-capstone/app/client/src/components/Navbar/Navbar.js",[],"/Users/macbook/Documents/mern-stack-capstone/app/client/src/pages/Dashboard/AddBlog.js",[],"/Users/macbook/Documents/mern-stack-capstone/app/client/src/icons/BoxFull.js",[],"/Users/macbook/Documents/mern-stack-capstone/app/client/src/util/helpers/recordCardHelpers.js",[],"/Users/macbook/Documents/mern-stack-capstone/app/client/src/components/App/theme.js",["235"],"/Users/macbook/Documents/mern-stack-capstone/app/client/src/pages/Home/HomeStyles.js",[],"/Users/macbook/Documents/mern-stack-capstone/app/client/src/pages/News/NewsCard.js",["236","237","238"],"/Users/macbook/Documents/mern-