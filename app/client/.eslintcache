    },\n    shoppingButton: {\n      border: `2px solid ${theme.palette.light.main}`,\n      borderRadius: 0,\n      marginTop: \"1rem\",\n      width: \"100%\",\n    },\n  };\n});\n\nexport default function CartTotals({ setShowCardForm }) {\n  const classes = useStyles();\n  const {\n    cartState: { cart, customer },\n  } = useCart();\n  const theme = useTheme();\n  const {\n    palette: { fluro },\n  } = theme;\n\n  return (\n    <div className={classes.card}>\n      <div className={classes.totalContainer}>\n        <h1 className={classes.totalsTitle}>total</h1>\n        <h1 className={classes.totalPrice}>\n          ${toCurrencyString(evaluateTotalPrice(cart))}\n        </h1>\n      </div>\n      <ButtonMain\n        disabled={!customer && true}\n        handleClick={() => setShowCardForm(true)}\n        color={fluro.main}\n      >\n        {!customer ? \"No shipping details\" : \"Checkout\"}\n      </ButtonMain>\n      <Link to=\"/\">\n        <ButtonMain fullWidth color={fluro.main}>\n          Continue Shopping\n        </ButtonMain>\n      </Link>\n      {/* <Button\n        className={classes.checkoutButton}\n        onClick={() => {\n          console.log({ cartState });\n        }}\n      >\n        get cart state\n      </Button> */}\n    </div>\n  );\n}\n",["263","264"],"/Users/macbook/Documents/mern-stack-capstone/app/client/src/components/MainContainer/MainContainerStyles.js",[],"/Users/macbook/Documents/mern-stack-capstone/app/client/src/context/reducers/cartReducer.js",[],["265","266"],"/Users/macbook/Documents/mern-stack-capstone/app/client/src/pages/SignUp/SignUpStyles.js",[],["267","268"],"/Users/macbook/Documents/mern-stack-capstone/app/client/src/pages/Cart/Checkout.js",[],"/Users/macbook/Documents/mern-stack-capstone/app/client/src/pages/Dashboard/AddRecords.js",[],"/Users/macbook/Documents/mern-stack-capstone/app/client/src/pages/Dashboard/EditBlog.js",[],"/Users/macbook/Documents/mern-stack-capstone/app/client/src/icons/BoxEmpty.js",[],"/Use