  <span role=\"alert\" className={classes.errorMessage}>\n                This is required\n              </span>\n            )}\n            {errors.password && errors.password.type === \"minLength\" && (\n              <span role=\"alert\" className={classes.errorMessage}>\n                Minimum 7 characters\n              </span>\n            )}\n          </div>\n          <input\n            className={classes.submitButton}\n            type=\"submit\"\n            value=\"Submit\"\n            name=\"submit\"\n            aria-label=\"Submit\"\n          />\n        </form>\n        <span className={classes.signUpMessage}>\n          Don't have an account?\n          <Link\n            to=\"/signup\"\n            className={classes.bottomLinks}\n            color=\"secondary\"\n            onClick={() => globe.setLoginModalState(false)}\n          >\n            Sign Up\n          </Link>\n        </span>\n        <Link to=\"/\" className={classes.bottomLinks} color=\"secondary\">\n          Reset Password\n        </Link>\n      </div>\n    </React.Fragment>\n  );\n}\n","/Users/macbook/Documents/mern-stack-capstone/app/client/src/util/helpers/newsHelpers.js",[],["214","215"],"/Users/macbook/Documents/mern-stack-capstone/app/client/src/components/RecordCard/RecordCardStyles.js",[],["216","217"],"/Users/macbook/Documents/mern-stack-capstone/app/client/src/context/AuthContext.js",[],"/Users/macbook/Documents/mern-stack-capstone/app/client/src/components/Footer/Footer.js",[],"/Users/macbook/Documents/mern-stack-capstone/app/client/src/pages/Contact/Contact.js",["218"],"/Users/macbook/Documents/mern-stack-capstone/app/client/src/context/GlobalState.js",[],["219","220"],"/Users/macbook/Documents/mern-stack-capstone/app/client/src/pages/SignUp/SignUp.js",[],"/Users/macbook/Documents/mern-stack-capstone/app/client/src/pages/Dashboard/Dashboard.js",[],"/Users/macbook/Documents/mern-stack-capstone/app/client/src/pages/SearchResults/SearchResults.js",[],"/Users/macbo