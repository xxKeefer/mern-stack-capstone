MenuDrawer } = globe;\n\n  const ListItemLink = function (props) {\n    const { onClick, primary, to } = props;\n\n    const CustomLink = React.useMemo(\n      () =>\n        React.forwardRef((linkProps, ref) => (\n          <Link ref={ref} to={to} {...linkProps} />\n        )),\n      [to]\n    );\n\n    return (\n      <li>\n        <ListItem\n          className={classes.listItems}\n          button\n          component={CustomLink}\n          onClick={onClick}\n        >\n          <ListItemText\n            classes={{ primary: classes.listItemText }}\n            inset={true}\n            primary={primary}\n          />\n        </ListItem>\n      </li>\n    );\n  };\n\n  return (\n    <React.Fragment>\n      <IconButton\n        aria-label=\"menu\"\n        edge=\"start\"\n        onClick={() => {\n          setMenuDrawer(!menuDrawer);\n        }}\n      >\n        <MenuIcon className={classes.menuIcon} />\n      </IconButton>\n\n      <Drawer\n        className={classes.root}\n        variant=\"temporary\"\n        anchor=\"left\"\n        open={menuDrawer}\n        classes={{ paper: classes.paper }}\n      >\n        <List style={{ padding: \"0px\" }}>\n          <ListItem className={classes.toolBarContainer} key=\"toolBarContainer\">\n            <ToolBarMenuDrawer />\n          </ListItem>\n          <ListItem\n            className={classes.searchListItem}\n            alignItems=\"center\"\n            key=\"searchListItem\"\n          >\n            <SearchField />\n          </ListItem>\n          {auth.authState ? (\n            <ListItemLink\n              to=\"/account\"\n              primary=\"my account\"\n              onClick={() => setMenuDrawer(false)}\n            />\n          ) : (\n            <ListItemLink\n              primary=\"log in\"\n              onClick={() => globe.setLoginModalState(true)}\n            />\n          )}\n\n          <ListItemLink\n            to=\"/new\"\n            primary=\"new vinyl\"\n            onClick={